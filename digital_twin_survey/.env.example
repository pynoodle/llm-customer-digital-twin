# OpenAI API 설정
# https://platform.openai.com/api-keys 에서 API 키를 발급받으세요
OPENAI_API_KEY=your_openai_api_key_here

# 사용할 모델 (선택사항)
# gpt-4o-mini: 빠르고 저렴 (권장)
# gpt-4o: 더 정확하지만 비용 높음
MODEL_NAME=gpt-4o-mini

# API 요청 설정
# 요청 간 대기 시간 (초)
REQUEST_DELAY=0.5

# 최대 재시도 횟수
MAX_RETRIES=3

# 프로젝트 설정
# 결과 저장 디렉토리
RESULTS_DIR=./results

# 로그 레벨 (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
